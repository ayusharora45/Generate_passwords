    "#python program to generate password\n",
    "\n",
    "import random\n",
    "lower = \"abcdefghijklmnopqrstuvwxyz\"\n",
    "upper = \"ABCDEFGHIJKLMNOPQRSTUVWXYZ\"\n",
    "numbers = \"1234567890\"\n",
    "special = \"!@#$%^&*\"\n",
    "\n",
    "all=lower+upper+numbers+special\n",
    "length=8\n",
    "password=\"\".join(random.sample(all,length))\n",
    "print(password)"
